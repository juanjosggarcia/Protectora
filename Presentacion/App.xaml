<Application x:Class="Protectora.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Protectora"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="RecursosEstilos" Source="Estilos.xaml" />
                <ResourceDictionary Source="/Protectora;component/idiomas/StringResources.es-ES.xaml" />
                <ResourceDictionary Source="/Protectora;component/idiomas/StringResources.en-UK.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
